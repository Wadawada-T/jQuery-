    $(function(){
        //无参遍历
       /* $("ul li").each(function () {
            alert($(this).text())
        })*/

       //有参遍历
       /* $("ul li").each(function (index) {
            alert($(this).eq(index).text())
        })*/

        //两个参数遍历
       /* $("ul li").each(function (index,e) {
            alert(e.innerText)
        })*/

       $.each($("li"),function () {
           alert($(this).text())
       })
        //有参同上


    })
</script>
